{"ast":null,"code":"import backgroundUrl from \"../assets/images/LogInSignUpDesktop.jpg\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      backgroundUrl,\n      email: '',\n      password: '',\n      loginSuccess: false\n    };\n  },\n\n  methods: {\n    onLogin() {\n      axios.post('https://jsonplaceholder.typicode.com/users', {\n        email: this.email,\n        password: this.password\n      }).then(response => {\n        this.loginSuccess = true;\n        console.log(response);\n        console.log(this.email + ' ' + this.password); //if login form is validated, this directs the user to the landing page.\n\n        if (this.loginSuccess == true) {\n          window.location.href = \"/\";\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA+BE,OAAOA,aAAP,MAA0B,yCAA1B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLF,aADK;MAELG,KAAK,EAAC,EAFD;MAGLC,QAAQ,EAAC,EAHJ;MAILC,YAAY,EAAE;IAJT,CAAP;EAMD,CARY;;EASbC,OAAO,EAAE;IACPC,OAAO,GAAG;MACRN,KAAK,CAACO,IAAN,CAAW,4CAAX,EACA;QAACL,KAAK,EAAE,KAAKA,KAAb;QAAoBC,QAAQ,EAAE,KAAKA;MAAnC,CADA,EAEEK,IAFF,CAEOC,QAAO,IAAK;QACjB,KAAKL,YAAL,GAAoB,IAApB;QACAM,OAAO,CAACC,GAAR,CAAYF,QAAZ;QACAC,OAAO,CAACC,GAAR,CAAY,KAAKT,KAAL,GAAa,GAAb,GAAmB,KAAKC,QAApC,EAHiB,CAKjB;;QACA,IAAI,KAAKC,YAAL,IAAqB,IAAzB,EAA8B;UAC5BQ,MAAM,CAACC,QAAP,CAAgBC,IAAhB,GAAuB,GAAvB;QACF;MACD,CAXD;IAYF;;EAdO;AATI,CAAf","names":["backgroundUrl","axios","data","email","password","loginSuccess","methods","onLogin","post","then","response","console","log","window","location","href"],"sourceRoot":"","sources":["C:\\yoobee_JS_Framework_Assignment\\src\\views\\LoginView.vue"],"sourcesContent":["<template>\r\n  <div class=\"background-image\" :style=\"{ backgroundImage: 'url(' + backgroundUrl + ')' }\">\r\n    <div class=\"login\">\r\n    <h1>Login</h1>  \r\n\r\n    <form class=\"loginForm\" @submit.prevent=\"onLogin\">\r\n      <div class=\"loginForm__container\">\r\n        <input class=\"loginFormInput\" type=\"email\" placeholder=\"Email Address\" name=\"email\" v-model=\"email\" required>\r\n        <input class=\"loginFormInput\" type=\"password\" placeholder=\"Password\" name=\"password\" v-model=\"password\" required>\r\n            \r\n        <button type=\"submit\" id=\"submitLogin\">Login</button>\r\n      </div>\r\n\r\n      <div class=\"loginForm__container\">\r\n        <span class=\"signUp\"><a href=\"/register\">Don't have an account?</a></span>\r\n        <span class=\"psw\"><a href=\"/register\">Forgot password?</a></span>\r\n      </div>\r\n    </form>\r\n    </div>\r\n    <div class=\"alert\" v-if=\"loginSuccess\">Logged in successfully</div>\r\n  </div>\r\n</template>\r\n\r\n\r\n\r\n<style>\r\n\r\n@import '../assets/styles/main.css';\r\n</style>\r\n\r\n<script>\r\n  import backgroundUrl from \"../assets/images/LogInSignUpDesktop.jpg\";\r\n  import axios from 'axios';\r\n  export default {\r\n    data() {\r\n      return {\r\n        backgroundUrl,\r\n        email:'',\r\n        password:'',\r\n        loginSuccess: false,\r\n      };\r\n    },\r\n    methods: {\r\n      onLogin() {\r\n        axios.post('https://jsonplaceholder.typicode.com/users',\r\n        {email: this.email, password: this.password },\r\n        ).then(response => {\r\n          this.loginSuccess = true;\r\n          console.log(response);\r\n          console.log(this.email + ' ' + this.password);\r\n\r\n          //if login form is validated, this directs the user to the landing page.\r\n          if (this.loginSuccess == true){\r\n            window.location.href = \"/\";\r\n          }\r\n        });\r\n      }\r\n\r\n    }\r\n  };\r\n</script>"]},"metadata":{},"sourceType":"module"}