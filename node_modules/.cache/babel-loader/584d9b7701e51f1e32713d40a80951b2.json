{"ast":null,"code":"import Axios from \"axios\";\nimport ProfileForm from \"@/components/ProfileForm.vue\";\nimport HelloWorld from '@/components/HelloWorld.vue';\nconst cloudName = \"dqyhsobqa\";\nconst unsignedUploadPreset = \"ewvntos9\";\nconst cloudinaryLink = `https://api.cloudinary.com/v1_1/${cloudName}/upload`;\nexport default {\n  name: \"App\",\n  components: {\n    ProfileForm,\n    Hello\n  },\n\n  data() {\n    return {\n      src: \"\",\n      uploading: false\n    };\n  },\n\n  methods: {\n    uploadFile(fileData) {\n      this.uploading = true;\n      const formData = new FormData();\n      formData.append(\"file\", fileData);\n      formData.append(\"upload_preset\", unsignedUploadPreset);\n      Axios.post(cloudinaryLink, formData).then(res => {\n        this.src = res.data.url;\n        this.uploading = false;\n      });\n    },\n\n    removeImage() {\n      this.src = \"\";\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAuDA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,WAAP,MAAwB,8BAAxB;AACA,OAAOC,UAAP,MAAuB,6BAAvB;AAEA,MAAMC,SAAQ,GAAI,WAAlB;AACA,MAAMC,oBAAmB,GAAI,UAA7B;AACA,MAAMC,cAAa,GAAK,mCAAkCF,SAAU,SAApE;AAEA,eAAe;EACbG,IAAI,EAAE,KADO;EAEbC,UAAU,EAAE;IACVN,WADU;IAEVO;EAFU,CAFC;;EAMbC,IAAI,GAAG;IACL,OAAO;MACLC,GAAG,EAAE,EADA;MAELC,SAAS,EAAE;IAFN,CAAP;EAID,CAXY;;EAYbC,OAAO,EAAE;IACPC,UAAU,CAACC,QAAD,EAAW;MACnB,KAAKH,SAAL,GAAiB,IAAjB;MACA,MAAMI,QAAO,GAAI,IAAIC,QAAJ,EAAjB;MACAD,QAAQ,CAACE,MAAT,CAAgB,MAAhB,EAAwBH,QAAxB;MACAC,QAAQ,CAACE,MAAT,CAAgB,eAAhB,EAAiCb,oBAAjC;MACAJ,KAAK,CAACkB,IAAN,CAAWb,cAAX,EAA2BU,QAA3B,EAAqCI,IAArC,CAA0CC,GAAE,IAAK;QAC/C,KAAKV,GAAL,GAAWU,GAAG,CAACX,IAAJ,CAASY,GAApB;QACA,KAAKV,SAAL,GAAiB,KAAjB;MACD,CAHD;IAID,CAVM;;IAWPW,WAAW,GAAG;MACZ,KAAKZ,GAAL,GAAW,EAAX;IACF;;EAbO;AAZI,CAAf","names":["Axios","ProfileForm","HelloWorld","cloudName","unsignedUploadPreset","cloudinaryLink","name","components","Hello","data","src","uploading","methods","uploadFile","fileData","formData","FormData","append","post","then","res","url","removeImage"],"sourceRoot":"","sources":["C:\\yoobee_JS_Framework_Assignment\\src\\views\\ProfileView.vue"],"sourcesContent":["<template>\r\n  \r\n    <ProfileForm\r\n      @image-upload=\"uploadFile\"\r\n      :src=\"src\"\r\n      :isUploading=\"uploading\"\r\n      @remove-image=\"removeImage\"\r\n    />\r\n    \r\n     <br><br>\r\n<p>Username: {{ message }}</p>\r\n<input v-model=\"message\" placeholder=\"Username\" />\r\n<br><br>\r\n<span>Bio:</span>\r\n<p style=\"white-space: pre-line;\">{{ message }}</p>\r\n<textarea v-model=\"message\" placeholder=\"I love to create event\"></textarea>\r\n\r\n<br><br>\r\n\r\n<router-link to=\"/create\"><button id=\"createEvent\" class=\"center\">Create a new event</button></router-link>\r\n<br><br>\r\n\r\n<h2>Contacts</h2>\r\n\r\n<table class=\"center\">\r\n  <tr>\r\n    <th>Person</th>\r\n    <th>Phone</th>\r\n    <th>Email</th>\r\n  </tr>\r\n  <tr>\r\n    <td>Justina Ho</td>\r\n    <td>021345678</td>\r\n    <td><a href=\"mailto:justina@gmail.com\">justina@gmail.com</a></td>\r\n  </tr>\r\n  <tr>\r\n    <td>Keegan Dufty</td>\r\n    <td>0212345678</td>\r\n    <td><a href=\"mailto:keegan@hotmail.com\">keegan@hotmail.com</a></td>\r\n  </tr>\r\n  <tr>\r\n    <td>Thomas King</td>\r\n    <td>0274567832</td>\r\n    <td><a href=\"mailto:thomasking135@gmail.com\">thomas@gmail.com</a></td>\r\n  </tr>\r\n  \r\n</table>\r\n\r\n\r\n<FooterSection/> \r\n</template>\r\n\r\n<script>\r\n\r\n\r\nimport Axios from \"axios\";\r\nimport ProfileForm from \"@/components/ProfileForm.vue\";\r\nimport HelloWorld from '@/components/HelloWorld.vue';\r\n\r\nconst cloudName = \"dqyhsobqa\";\r\nconst unsignedUploadPreset = \"ewvntos9\";\r\nconst cloudinaryLink = `https://api.cloudinary.com/v1_1/${cloudName}/upload`;\r\n\r\nexport default {\r\n  name: \"App\",\r\n  components: {\r\n    ProfileForm,\r\n    Hello\r\n},\r\n  data() {\r\n    return {\r\n      src: \"\",\r\n      uploading: false\r\n    };\r\n  },\r\n  methods: {\r\n    uploadFile(fileData) {\r\n      this.uploading = true;\r\n      const formData = new FormData();\r\n      formData.append(\"file\", fileData);\r\n      formData.append(\"upload_preset\", unsignedUploadPreset);\r\n      Axios.post(cloudinaryLink, formData).then(res => {\r\n        this.src = res.data.url;\r\n        this.uploading = false;\r\n      });\r\n    },\r\n    removeImage() {\r\n      this.src = \"\";\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n  table {\r\n    padding-top: 10px;\r\n    font-family: arial, sans-serif;\r\n    border-collapse: collapse black 1px;\r\n    width: 80%;\r\n    left: 50%;\r\n    \r\n  }\r\n\r\n  td, th {\r\n  border: 1px solid #dddddd;\r\n  text-align: center;\r\n  padding: 8px;\r\n}\r\n\r\ntr:nth-child(even) {\r\n  background-color: #dddddd;\r\n}\r\n\r\n.center {\r\n  margin-left: auto;\r\n  margin-right: auto;\r\n}\r\n\r\n#createEvent {\r\n  background-color: #08bdb6;\r\n  text-align: center;\r\n  font-size: 14px;\r\n  font-family: lato, sans-serif;\r\n  font-weight: 500;\r\n  font-style: normal;\r\n  color: #303030;\r\n  padding-top: 10px;\r\n  padding-bottom: 10px;\r\n  padding-left: 50px;\r\n  padding-right: 50px;\r\n}\r\n\r\n#createEvent:hover {\r\n  background-color: #48b1bf;\r\n  border-radius: 26px;\r\n}\r\n</style>\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n"]},"metadata":{},"sourceType":"module"}