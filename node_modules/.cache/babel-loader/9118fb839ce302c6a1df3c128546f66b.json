{"ast":null,"code":"import backgroundUrl from \"../assets/images/LogInSignUpDesktop.jpg\";\nimport axios from \"axios\";\nconst baseURL = \"http://localhost:3000/users\";\nexport default {\n  data() {\n    return {\n      backgroundUrl,\n      email: '',\n      username: '',\n      formIsValid: true,\n      users: [],\n      userAdded: false,\n      nameTaken: false\n    };\n  },\n\n  async created() {\n    try {\n      const res = await axios.get(baseURL);\n      this.users = res.data;\n    } catch (e) {\n      console.error(e);\n    }\n  },\n\n  methods: {\n    onRegister() {\n      this.formIsValid = true;\n\n      if (this.email === \"\" || !this.email.includes(\"@\") || this.username.length < 1) {\n        this.formIsValid = false;\n        return;\n      }\n    },\n\n    async addUser() {\n      if (this.username === 'justina' || this.username === 'keegan' || this.username === 'thom') {\n        this.nameTaken = true;\n      } else {\n        const res = await axios.post(baseURL, {\n          email: this.email,\n          username: this.username\n        });\n        this.users = [...this.users, res.data];\n        this.username = '';\n        this.email = '';\n        this.userAdded == true;\n        window.location.href = \"/login\";\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA8CA,OAAOA,aAAP,MAA0B,yCAA1B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,MAAMC,OAAM,GAAI,6BAAhB;AAEA,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLH,aADK;MAELI,KAAK,EAAE,EAFF;MAGLC,QAAQ,EAAE,EAHL;MAILC,WAAW,EAAE,IAJR;MAKLC,KAAK,EAAE,EALF;MAMLC,SAAS,EAAE,KANN;MAOLC,SAAS,EAAE;IAPN,CAAP;EASD,CAXY;;EAYZ,MAAMC,OAAN,GAAe;IACd,IAAI;MACF,MAAMC,GAAE,GAAI,MAAMV,KAAK,CAACW,GAAN,CAAUV,OAAV,CAAlB;MACA,KAAKK,KAAL,GAAaI,GAAG,CAACR,IAAjB;IACF,CAHA,CAGE,OAAMU,CAAN,EAAQ;MACRC,OAAO,CAACC,KAAR,CAAcF,CAAd;IACF;EACD,CAnBY;;EAoBbG,OAAO,EAAE;IACPC,UAAU,GAAG;MACX,KAAKX,WAAL,GAAmB,IAAnB;;MACA,IACE,KAAKF,KAAL,KAAe,EAAf,IACA,CAAC,KAAKA,KAAL,CAAWc,QAAX,CAAoB,GAApB,CADD,IAEA,KAAKb,QAAL,CAAcc,MAAd,GAAuB,CAHzB,EAIE;QACA,KAAKb,WAAL,GAAmB,KAAnB;QACA;MACF;IAED,CAZM;;IAcL,MAAMc,OAAN,GAAgB;MAEhB,IAAI,KAAKf,QAAL,KAAkB,SAAlB,IAA+B,KAAKA,QAAL,KAAkB,QAAjD,IAA6D,KAAKA,QAAL,KAAkB,MAAnF,EAA0F;QACxF,KAAKI,SAAL,GAAiB,IAAjB;MACF,CAFA,MAEO;QACL,MAAME,GAAE,GAAI,MAAMV,KAAK,CAACoB,IAAN,CAAWnB,OAAX,EAAoB;UAAEE,KAAK,EAAE,KAAKA,KAAd;UAAqBC,QAAQ,EAAE,KAAKA;QAApC,CAApB,CAAlB;QACA,KAAKE,KAAL,GAAa,CAAC,GAAG,KAAKA,KAAT,EAAgBI,GAAG,CAACR,IAApB,CAAb;QACA,KAAKE,QAAL,GAAgB,EAAhB;QACA,KAAKD,KAAL,GAAa,EAAb;QACA,KAAKI,SAAL,IAAkB,IAAlB;QACAc,MAAM,CAACC,QAAP,CAAgBC,IAAhB,GAAuB,QAAvB;MACF;IACF;;EA1BO;AApBI,CAAf","names":["backgroundUrl","axios","baseURL","data","email","username","formIsValid","users","userAdded","nameTaken","created","res","get","e","console","error","methods","onRegister","includes","length","addUser","post","window","location","href"],"sourceRoot":"","sources":["C:\\yoobee_JS_Framework_Assignment\\src\\views\\RegisterView.vue"],"sourcesContent":["<template>\r\n  <div\r\n    class=\"background-image\"\r\n    :style=\"{ backgroundImage: 'url(' + backgroundUrl + ')' }\"\r\n  >\r\n    <div class=\"register\">\r\n      <h1>Sign Up</h1>\r\n      <form class=\"registerForm\" @submit.prevent=\"onRegister\" novalidate>\r\n        <div class=\"registerForm__container\">\r\n          <input\r\n            class=\"registerFormInput\"\r\n            type=\"email\"\r\n            placeholder=\"Email Address\"\r\n            name=\"email\"\r\n            v-model=\"email\"\r\n            required\r\n          />\r\n          <input\r\n            class=\"registerFormInput\"\r\n            type=\"username\"\r\n            placeholder=\"Username\"\r\n            name=\"username\"\r\n            v-model=\"username\" @keyup.enter=\"addUser\"\r\n            required\r\n          />\r\n\r\n          <button type=\"submit\" id=\"submitSignUp\">Sign Up</button>\r\n        </div>\r\n\r\n        <div class=\"registerForm__container\">\r\n          <span class=\"account\"\r\n            ><a href=\"/login\">Already have an account?</a></span\r\n          >\r\n        </div>\r\n      </form>\r\n    </div>\r\n    <div v-if=\"!formIsValid\">Please enter a valid email and username</div>\r\n     <div v-if=\"nameTaken\">This username has been taken, please try another one.</div>\r\n  </div>\r\n</template>\r\n\r\n<style>\r\n@import \"../assets/styles/main.css\";\r\n</style>\r\n\r\n<script>\r\nimport backgroundUrl from \"../assets/images/LogInSignUpDesktop.jpg\";\r\nimport axios from \"axios\";\r\nconst baseURL = \"http://localhost:3000/users\"\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      backgroundUrl,\r\n      email: '',\r\n      username: '',\r\n      formIsValid: true,\r\n      users: [],\r\n      userAdded: false,\r\n      nameTaken: false\r\n    };\r\n  },\r\n   async created(){\r\n    try {\r\n      const res = await axios.get(baseURL);\r\n      this.users = res.data;\r\n    } catch(e){\r\n      console.error(e);\r\n    }\r\n  },\r\n  methods: {\r\n    onRegister() {\r\n      this.formIsValid = true;\r\n      if (\r\n        this.email === \"\" ||\r\n        !this.email.includes(\"@\") ||\r\n        this.username.length < 1\r\n      ) {\r\n        this.formIsValid = false;\r\n        return;\r\n      }\r\n      \r\n    },\r\n\r\n      async addUser() {\r\n\r\n      if (this.username === 'justina' || this.username === 'keegan' || this.username === 'thom'){\r\n        this.nameTaken = true;\r\n      } else {\r\n        const res = await axios.post(baseURL, { email: this.email, username: this.username });\r\n        this.users = [...this.users, res.data];\r\n        this.username = '';\r\n        this.email = '';\r\n        this.userAdded == true\r\n        window.location.href = \"/login\";\r\n      } \r\n    }\r\n    \r\n  },\r\n};\r\n</script>\r\n"]},"metadata":{},"sourceType":"module"}