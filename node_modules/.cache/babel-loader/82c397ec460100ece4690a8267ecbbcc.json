{"ast":null,"code":"import backgroundUrl from \"../assets/images/LogInSignUpDesktop.jpg\";\nimport axios from \"axios\";\nconst baseURL = \"http://localhost:3000/users\";\nexport default {\n  data() {\n    return {\n      backgroundUrl,\n      email: '',\n      username: '',\n      formIsValid: true,\n      users: [],\n      usersName: ''\n    };\n  },\n\n  async created() {\n    try {\n      const res = await axios.get(baseURL);\n      this.users = res.data;\n    } catch (e) {\n      console.error(e);\n    }\n  },\n\n  methods: {\n    onRegister() {\n      this.formIsValid = true;\n\n      if (this.email === \"\" || !this.email.includes(\"@\") || this.username.length < 1) {\n        this.formIsValid = false;\n        return;\n      }\n\n      axios.post(\"http://localhost:3000/users\", {\n        email: this.email,\n        username: this.username\n      }).then(response => {\n        console.log(response);\n      }, error => {\n        console.log(error);\n      });\n    },\n\n    async addUser() {\n      const res = await axios.post(baseURL, {\n        name: this.usersName\n      });\n      this.users = [...this.users, res.data];\n      this.usersName = '';\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA8CA,OAAOA,aAAP,MAA0B,yCAA1B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,MAAMC,OAAM,GAAI,6BAAhB;AAEA,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLH,aADK;MAELI,KAAK,EAAE,EAFF;MAGLC,QAAQ,EAAE,EAHL;MAILC,WAAW,EAAE,IAJR;MAKLC,KAAK,EAAE,EALF;MAMLC,SAAS,EAAE;IANN,CAAP;EAQD,CAVY;;EAWZ,MAAMC,OAAN,GAAe;IACd,IAAI;MACF,MAAMC,GAAE,GAAI,MAAMT,KAAK,CAACU,GAAN,CAAUT,OAAV,CAAlB;MACA,KAAKK,KAAL,GAAaG,GAAG,CAACP,IAAjB;IACF,CAHA,CAGE,OAAMS,CAAN,EAAQ;MACRC,OAAO,CAACC,KAAR,CAAcF,CAAd;IACF;EACD,CAlBY;;EAmBbG,OAAO,EAAE;IACPC,UAAU,GAAG;MACX,KAAKV,WAAL,GAAmB,IAAnB;;MACA,IACE,KAAKF,KAAL,KAAe,EAAf,IACA,CAAC,KAAKA,KAAL,CAAWa,QAAX,CAAoB,GAApB,CADD,IAEA,KAAKZ,QAAL,CAAca,MAAd,GAAuB,CAHzB,EAIE;QACA,KAAKZ,WAAL,GAAmB,KAAnB;QACA;MACF;;MAEAL,KAAI,CACDkB,IADH,CACQ,6BADR,EACuC;QACnCf,KAAK,EAAE,KAAKA,KADuB;QAEnCC,QAAQ,