{"ast":null,"code":"import Vue from \"vue\";\nimport axios from \"axios\";\nimport VueAxios from \"vue-axios\";\nVue.use(VueAxios, axios);\nconst app = Vue.createApp({\n  name: \"comments\",\n\n  data() {\n    return {\n      comments: null\n    };\n  },\n\n  methods: {\n    getData() {\n      this.axios.get(\"http://localhost:3000/comments\").then(response => {\n        this.users = response.data;\n        console.log(response);\n        this.comments = response.data;\n      });\n    },\n\n    deleteUser(id) {\n      this.axios.delete(\"http://localhost:3000/comments\" + id).then(() => {\n        this.getData();\n      });\n    }\n\n  },\n\n  mounted() {\n    this.getData();\n  }\n\n});\napp.mount(\"#app\");","map":{"version":3,"mappings":"AAkBA,OAAOA,GAAP,MAAgB,KAAhB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,OAAOC,QAAP,MAAqB,WAArB;AAEAF,GAAG,CAACG,GAAJ,CAAQD,QAAR,EAAkBD,KAAlB;AAEA,MAAMG,GAAE,GAAIJ,GAAG,CAACK,SAAJ,CAAc;EACxBC,IAAI,EAAE,UADkB;;EAExBC,IAAI,GAAG;IACL,OAAO;MACLC,QAAQ,EAAE;IADL,CAAP;EAGD,CANuB;;EAQxBC,OAAO,EAAE;IACPC,OAAO,GAAG;MACR,KAAKT,KAAL,CAAWU,GAAX,CAAe,gCAAf,EAAiDC,IAAjD,CAAuDC,QAAD,IAAc;QAClE,KAAKC,KAAL,GAAaD,QAAQ,CAACN,IAAtB;QACAQ,OAAO,CAACC,GAAR,CAAYH,QAAZ;QACA,KAAKL,QAAL,GAAgBK,QAAQ,CAACN,IAAzB;MACD,CAJD;IAKD,CAPM;;IAQPU,UAAU,CAACC,EAAD,EAAK;MACb,KAAKjB,KAAL,CAAWkB,MAAX,CAAkB,mCAAmCD,EAArD,EAAyDN,IAAzD,CAA8D,MAAM;QAClE,KAAKF,OAAL;MACD,CAFD;IAGD;;EAZM,CARe;;EAsBxBU,OAAO,GAAG;IACR,KAAKV,OAAL;EACD;;AAxBuB,CAAd,CAAZ;AA2BAN,GAAG,CAACiB,KAAJ,CAAU,MAAV","names":["Vue","axios","VueAxios","use","app","createApp","name","data","comments","methods","getData","get","then","response","users","console","log","deleteUser","id","delete","mounted","mount"],"sourceRoot":"","sources":["/Users/justinaho1996/yoobee_JS_Framework_Assignment/src/components/Users.vue"],"sourcesContent":["<template>\n  <div>\n    <table class=\"commentPosts\" border=\"1px\">\n      <tr>\n        <td>name</td>\n        <td>post</td>\n        <td>action</td>\n      </tr>\n      <tr v-for=\"comment in comments\" v-bind:key=\"comment.id\">\n        <td>{{ comment.name }}</td>\n        <td>{{ comment.post }}</td>\n        <td><button v-on:click=\"deleteUser(comment.id)\">Delete</button></td>\n      </tr>\n    </table>\n  </div>\n</template>\n\n<script>\nimport Vue from \"vue\";\nimport axios from \"axios\";\nimport VueAxios from \"vue-axios\";\n\nVue.use(VueAxios, axios);\n\nconst app = Vue.createApp({\n  name: \"comments\",\n  data() {\n    return {\n      comments: null,\n    };\n  },\n\n  methods: {\n    getData() {\n      this.axios.get(\"http://localhost:3000/comments\").then((response) => {\n        this.users = response.data;\n        console.log(response);\n        this.comments = response.data;\n      });\n    },\n    deleteUser(id) {\n      this.axios.delete(\"http://localhost:3000/comments\" + id).then(() => {\n        this.getData();\n      });\n    },\n  },\n  mounted() {\n    this.getData();\n  },\n});\n\napp.mount(\"#app\");\n</script>\n\n<style scoped>\n.commentPosts {\n  width: 50%;\n  margin-left: auto;\n  margin-right: auto;\n  margin-top: 50px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}