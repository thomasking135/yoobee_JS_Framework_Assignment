{"ast":null,"code":"import backgroundUrl from \"../assets/images/LogInSignUpDesktop.jpg\";\nimport axios from \"axios\"; // import userData from '/data/db.json'\n\nconst baseURL = \"http://localhost:3000/users\";\nexport default {\n  name: 'login',\n\n  data() {\n    return {\n      backgroundUrl,\n      email: \"\",\n      username: \"\",\n      // userData: userData,\n      users: [],\n      url: 'http://localhost:3000/users',\n      formIsValid: true,\n      userDetailsIsValid: true\n    };\n  },\n\n  mounted() {\n    axios.get(this.url).then(response => {\n      this.users = response.data;\n    });\n  },\n\n  methods: {\n    submitForm() {\n      this.formIsValid = true;\n\n      if (this.email === \"\" || !this.email.includes(\"@\") || this.username.length < 1) {\n        this.formIsValid = false;\n        return;\n      }\n\n      for (let i = 0; i < this.users.length; i++) {\n        if (this.username == this.users[i].username && this.email == this.users[i].email) {\n          window.location.href = \"/\";\n        } else {\n          this.userDetailsIsValid = false;\n          return;\n        }\n      } //GET\n\n\n      axios.get(baseURL).then(res => {\n        handleResult(res);\n      }).catch(err => console.log(err));\n\n      function handleResult(data) {\n        console.log(data);\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA+CA,OAAOA,aAAP,MAA0B,yCAA1B;AACA,OAAOC,KAAP,MAAkB,OAAlB,C,CACA;;AACA,MAAMC,OAAM,GAAI,6BAAhB;AACA,eAAe;EACbC,IAAI,EAAE,OADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLJ,aADK;MAELK,KAAK,EAAE,EAFF;MAGLC,QAAQ,EAAE,EAHL;MAIL;MACAC,KAAK,EAAE,EALF;MAMLC,GAAG,EAAE,6BANA;MAOLC,WAAW,EAAE,IAPR;MAQLC,kBAAkB,EAAE;IARf,CAAP;EAUD,CAbY;;EAcbC,OAAO,GAAG;IACRV,KAAK,CAACW,GAAN,CAAU,KAAKJ,GAAf,EAAoBK,IAApB,CAA0BC,QAAD,IAAc;MACrC,KAAKP,KAAL,GAAaO,QAAQ,CAACV,IAAtB;IACD,CAFD;EAGD,CAlBY;;EAmBbW,OAAO,EAAE;IACPC,UAAU,GAAG;MACX,KAAKP,WAAL,GAAmB,IAAnB;;MACA,IACE,KAAKJ,KAAL,KAAe,EAAf,IACA,CAAC,KAAKA,KAAL,CAAWY,QAAX,CAAoB,GAApB,CADD,IAEA,KAAKX,QAAL,CAAcY,M