{"ast":null,"code":"import backgroundUrl from \"../assets/images/LogInSignUpDesktop.jpg\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      backgroundUrl,\n      email: '',\n      password: '',\n      confirmedPassword: '',\n      registerSuccess: false,\n      registerFail: false\n    };\n  },\n\n  methods: {\n    onRegister() {\n      axios.post('https://jsonplaceholder.typicode.com/users', {\n        email: this.email,\n        password: this.password,\n        confirmedPassword: this.confirmedPassword\n      }).then(response => {\n        //if both passwords are the same and register form is validated, this directs the user to the login page.\n        if (this.password == this.confirmedPassword) {\n          this.registerSuccess = true;\n          this.registerFail = false;\n\n          if (this.registerSuccess == true) {\n            window.location.href = \"/login\";\n          }\n        } else {\n          this.registerFail = true;\n        }\n\n        console.log(response);\n        console.log(this.email + ' ' + this.password + ' ' + this.confirmedPassword);\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA+BA,OAAOA,aAAP,MAA0B,yCAA1B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLF,aADK;MAELG,KAAK,EAAC,EAFD;MAGLC,QAAQ,EAAC,EAHJ;MAILC,iBAAiB,EAAC,EAJb;MAKLC,eAAe,EAAE,KALZ;MAMLC,YAAY,EAAE;IANT,CAAP;EAQD,CAVY;;EAWbC,OAAO,EAAE;IACLC,UAAU,GAAG;MACXR,KAAK,CAACS,IAAN,CAAW,4CAAX,EACA;QAACP,KAAK,EAAE,KAAKA,KAAb;QAAoBC,QAAQ,EAAE,KAAKA,QAAnC;QAA6CC,iBAAiB,EAAE,KAAKA;MAArE,CADA,EAEEM,IAFF,CAEOC,QAAO,IAAK;QAEjB;QACA,IAAI,KAAKR,QAAL,IAAiB,KAAKC,iBAA1B,EAA4C;UAC1C,KAAKC,eAAL,GAAuB,IAAvB;UACA,KAAKC,YAAL,GAAoB,KAApB;;UACA,IAAI,KAAKD,eAAL,IAAwB,IAA5B,EAAiC;YAC/BO,MAAM,CAACC,QAAP,CAAgBC,IAAhB,GAAuB,QAAvB;UACF;QACF,CANA,MAMO;UACL,KAAKR,YAAL,GAAoB,IAApB;QACF;;QAEAS,OAAO,CAACC,GAAR,CAAYL,QAAZ;QACAI,OAAO,CAACC,GAAR,CAAY,KAAKd,KAAL,GAAa,GAAb,GAAmB,KAAKC,QAAxB,GAAmC,GAAnC,GAAyC,KAAKC,iBAA1D;MAED,CAlBD;IAmBF;;EArBK;AAXI,CAAf","names":["backgroundUrl","axios","data","email","password","confirmedPassword","registerSuccess","registerFail","methods","onRegister","post","then","response","window","location","href","console","log"],"sourceRoot":"","sources":["/Users/keegs/Documents/GitHub/yoobee_JS_Framework_Assignment/yoobee_JS_Framework_Assignment/src/views/RegisterView.vue"],"sourcesContent":["<template>\n  <div class=\"background-image\" :style=\"{ backgroundImage: 'url(' + backgroundUrl + ')' }\">\n    <div class=\"register\">\n      <h1>Sign Up</h1>  \n      <form class=\"registerForm\" @submit.prevent=\"onRegister\">\n        <div class=\"registerForm__container\">\n          <input class=\"registerFormInput\" type=\"email\" placeholder=\"Email Address\" name=\"email\" v-model=\"email\" required>\n          <input class=\"registerFormInput\" type=\"password\" placeholder=\"Password\" name=\"psw\" v-model=\"password\" required>\n          <input class=\"registerFormInput\" type=\"password\" placeholder=\"Confirm Password\" name=\"psw\" v-model=\"confirmedPassword\" required>\n              \n          <button type=\"submit\" id=\"submitSignUp\">Sign Up</button>\n        </div>\n\n        <div class=\"registerForm__container\">\n           <span class=\"account\"><a href=\"/login\">Already have an account?</a></span>\n        </div>\n        \n      </form>\n    </div>\n    <div class=\"alert\" v-if=\"registerSuccess\">Logged in successfully</div>\n    <div class=\"alert\" v-if=\"registerFail\">Error!</div>\n  </div>\n</template>\n\n\n\n<style>\n@import '../assets/styles/main.css';\n</style>\n\n<script>\nimport backgroundUrl from \"../assets/images/LogInSignUpDesktop.jpg\";\nimport axios from 'axios';\n\nexport default {\n  data() {\n    return {\n      backgroundUrl,\n      email:'',\n      password:'',\n      confirmedPassword:'',\n      registerSuccess: false,\n      registerFail: false,\n    };\n  },\n  methods: {\n      onRegister() {\n        axios.post('https://jsonplaceholder.typicode.com/users',\n        {email: this.email, password: this.password, confirmedPassword: this.confirmedPassword },\n        ).then(response => {\n          \n          //if both passwords are the same and register form is validated, this directs the user to the login page.\n          if (this.password == this.confirmedPassword){\n            this.registerSuccess = true;\n            this.registerFail = false;\n            if (this.registerSuccess == true){\n              window.location.href = \"/login\";\n            }\n          } else {\n            this.registerFail = true;\n          }\n\n          console.log(response);\n          console.log(this.email + ' ' + this.password + ' ' + this.confirmedPassword);\n    \n        });\n      }\n\n    }\n};\n</script>"]},"metadata":{},"sourceType":"module"}