{"ast":null,"code":"import backgroundUrl from \"../assets/images/LogInSignUpDesktop.jpg\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      backgroundUrl,\n      email: '',\n      password: '',\n      confirmedPassword: ''\n    };\n  },\n\n  methods: {\n    onCreatePost() {\n      axios.post('https://jsonplaceholder.typicode.com/users', {\n        email: this.email,\n        password: this.password,\n        confirmedPassword: this.confirmedPassword\n      }).then(response => {\n        this.isSuccess = true;\n        console.log(response);\n        console.log(this.email + ' ' + this.password + '' + this.confirmedPassword); //if login form is validated, this directs the user to the landing page.\n        // if (this.isSuccess == true){\n        //   window.location.href = \"/login\";\n        // }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA6BA,OAAOA,aAAP,MAA0B,yCAA1B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLF,aADK;MAELG,KAAK,EAAC,EAFD;MAGLC,QAAQ,EAAC,EAHJ;MAILC,iBAAiB,EAAC;IAJb,CAAP;EAMD,CARY;;EASbC,OAAO,EAAE;IACLC,YAAY,GAAG;MACbN,KAAK,CAACO,IAAN,CAAW,4CAAX,EACA;QAACL,KAAK,EAAE,KAAKA,KAAb;QAAoBC,QAAQ,EAAE,KAAKA,QAAnC;QAA6CC,iBAAiB,EAAE,KAAKA;MAArE,CADA,EAEEI,IAFF,CAEOC,QAAO,IAAK;QACjB,KAAKC,SAAL,GAAiB,IAAjB;QACAC,OAAO,CAACC,GAAR,CAAYH,QAAZ;QACAE,OAAO,CAACC,GAAR,CAAY,KAAKV,KAAL,GAAa,GAAb,GAAmB,KAAKC,QAAxB,GAAmC,EAAnC,GAAwC,KAAKC,iBAAzD,EAHiB,CAKjB;QACA;QACA;QACA;MACD,CAXD;IAYF;;EAdK;AATI,CAAf","names":["backgroundUrl","axios","data","email","password","confirmedPassword","methods","onCreatePost","post","then","response","isSuccess","console","log"],"sourceRoot":"","sources":["/Users/justinaho1996/yoobee_JS_Framework_Assignment/src/views/RegisterView.vue"],"sourcesContent":["<template>\n  <div class=\"background-image\" :style=\"{ backgroundImage: 'url(' + backgroundUrl + ')' }\">\n    <div class=\"register\">\n      <h1>Sign Up</h1>  \n      <form class=\"registerForm\">\n        <div class