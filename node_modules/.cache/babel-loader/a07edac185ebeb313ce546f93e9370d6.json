{"ast":null,"code":"import backgroundUrl from \"../assets/images/LogInSignUpDesktop.jpg\";\nimport axios from \"axios\";\nconst baseURL = \"http://localhost:3000/users\";\nexport default {\n  data() {\n    return {\n      backgroundUrl,\n      email: '',\n      username: '',\n      formIsValid: true,\n      users: [],\n      userAdded: false\n    };\n  },\n\n  async created() {\n    try {\n      const res = await axios.get(baseURL);\n      this.users = res.data;\n    } catch (e) {\n      console.error(e);\n    }\n  },\n\n  mounted() {\n    let user = localStorage.getItem('user-info');\n\n    if (user) {\n      this.$router.push({\n        name: 'Home'\n      });\n    }\n  },\n\n  methods: {\n    onRegister() {\n      this.formIsValid = true;\n\n      if (this.email === \"\" || !this.email.includes(\"@\") || this.username.length < 1) {\n        this.formIsValid = false;\n        return;\n      }\n    },\n\n    async addUser() {\n      const res = await axios.post(baseURL, {\n        email: this.email,\n        username: this.username\n      });\n      this.users = [...this.users, res.data];\n      this.username = '';\n      this.email = '';\n      this.userAdded = true;\n\n      if (this.userAdded == true) {\n        window.location.href = \"/login\";\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA8CA,OAAOA,aAAP,MAA0B,yCAA1B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,MAAMC,OAAM,GAAI,6BAAhB;AAEA,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLH,aADK;MAELI,KAAK,EAAE,EAFF;MAGLC,QAAQ,EAAE,EAHL;MAILC,WAAW,EAAE,IAJR;MAKLC,KAAK,EAAE,EALF;MAMLC,SAAS,EAAE;IANN,CAAP;EAQD,CAVY;;EAWZ,MAAMC,OAAN,GAAe;IACd,IAAI;MACF,MAAMC,GAAE,GAAI,MAAMT,KAAK,CAACU,GAAN,CAAUT,OAAV,CAAlB;MACA,KAAKK,KAAL,GAAaG,GAAG,CAACP,IAAjB;IACF,CAHA,CAGE,OAAMS,CAAN,EAAQ;MACRC,OAAO,CAACC,KAAR,CAAcF,CAAd;IACF;EACD,CAlBY;;EAmBbG,OAAO,GAAE;IACP,IAAIC,IAAG,GAAIC,YAAY,CAACC,OAAb,CAAqB,WAArB,CAAX;;IACA,IAAGF,IAAH,EAAS;MACP,KAAKG,OAAL,CAAaC,IAAb,CAAkB;QAACC,IAAI,EAAE;MAAP,CAAlB;IACF;EACD,CAxBY;;EAyBbC,OAAO,EAAE;IACPC,UAAU,GAAG;M