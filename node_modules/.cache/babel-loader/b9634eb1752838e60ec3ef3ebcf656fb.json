{"ast":null,"code":"import backgroundUrl from \"../assets/images/LogInSignUpDesktop.jpg\";\nimport axios from \"axios\";\nconst baseURL = \"http://localhost:3000/users\";\nexport default {\n  data() {\n    return {\n      backgroundUrl,\n      email: \"\",\n      username: \"\",\n      formIsValid: true\n    };\n  },\n\n  mounted() {\n    fetch(baseURL).then(res => res.json()).then(data => this.users = data).catch(err => console.log(err.message));\n  },\n\n  methods: {\n    async login() {\n      let result = await axios.get('http://localhost:3000/users?email=${this.email}&username=${this.username}');\n\n      if (result.status == 200 && result.data.length > 0) {\n        localStorage.setItem('user-info', JSON.stringify(result.data[0]));\n        this.$router.push({\n          name: 'Home'\n        });\n      }\n\n      console.warn(result);\n    },\n\n    submitForm() {\n      this.formIsValid = true;\n\n      if (this.email === \"\" || !this.email.includes(\"@\") || this.username.length < 1) {\n        this.formIsValid = false;\n        return;\n      } //GET\n\n\n      axios.get(baseURL).then(res => {\n        handleResult(res);\n      }).catch(err => console.log(err));\n\n      function handleResult(data) {\n        console.log(data);\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA8CA,OAAOA,aAAP,MAA0B,yCAA1B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,MAAMC,OAAM,GAAI,6BAAhB;AACA,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLH,aADK;MAELI,KAAK,EAAE,EAFF;MAGLC,QAAQ,EAAE,EAHL;MAILC,WAAW,EAAE;IAJR,CAAP;EAMD,CARY;;EASbC,OAAO,GAAG;IACRC,KAAK,CAACN,OAAD,CAAL,CACCO,IADD,CACMC,GAAE,IAAKA,GAAG,CAACC,IAAJ,EADb,EAECF,IAFD,CAEMN,IAAG,IAAK,KAAKS,KAAL,GAAaT,IAF3B,EAGCU,KAHD,CAGOC,GAAE,IAAKC,OAAO,CAACC,GAAR,CAAYF,GAAG,CAACG,OAAhB,CAHd;EAID,CAdY;;EAebC,OAAO,EAAE;IACP,MAAMC,KAAN,GAAc;MACZ,IAAIC,MAAK,GAAI,MAAMnB,KAAK,CAACoB,GAAN,CACjB,2EADiB,CAAnB;;MAIE,IAAID,MAAM,CAACE,MAAP,IAAe,GAAf,IAAsBF,MAAM,CAACjB,IAAP,CAAYoB,MAAZ,GAAmB,CAA7C,EAA+C;QAC7CC,YAAY,CAACC,OAAb,CAAqB,WAArB,EAAkCC,IAAI,CAACC