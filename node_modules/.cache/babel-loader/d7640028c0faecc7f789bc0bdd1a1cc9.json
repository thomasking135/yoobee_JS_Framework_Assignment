{"ast":null,"code":"import backgroundUrl from \"../assets/images/LogInSignUpDesktop.jpg\";\nimport axios from \"axios\";\nconst baseURL = \"http://localhost:3000/users\";\nexport default {\n  data() {\n    return {\n      backgroundUrl,\n      email: '',\n      username: '',\n      formIsValid: true,\n      users: [],\n      userAdded: false,\n      nameTaken: false\n    };\n  },\n\n  async created() {\n    try {\n      const res = await axios.get(baseURL);\n      this.users = res.data;\n    } catch (e) {\n      console.error(e);\n    }\n  },\n\n  methods: {\n    onRegister() {\n      this.formIsValid = true;\n\n      if (this.email === \"\" || !this.email.includes(\"@\") || this.username.length < 1) {\n        this.formIsValid = false;\n        return;\n      }\n    },\n\n    async addUser() {\n      const res = await axios.post(baseURL, {\n        email: this.email,\n        username: this.username\n      });\n      this.users = [...this.users, res.data];\n      this.username = '';\n      this.email = '';\n\n      if (this.username === 'justina' || this.username === 'keegan' || this.username === 'thom') {\n        this.nameTaken = true;\n      } else {\n        this.userAdded == true;\n        window.location.href = \"/login\";\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA8CA,OAAOA,aAAP,MAA0B,yCAA1B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,MAAMC,OAAM,GAAI,6BAAhB;AAEA,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLH,aADK;MAELI,KAAK,EAAE,EAFF;MAGLC,QAAQ,EAAE,EAHL;MAILC,WAAW,EAAE,IAJR;MAKLC,KAAK,EAAE,EALF;MAMLC,SAAS,EAAE,KANN;MAOLC,SAAS,EAAE;IAPN,CAAP;EASD,CAXY;;EAYZ,MAAMC,OAAN,GAAe;IACd,IAAI;MACF,MAAMC,GAAE,GAAI,MAAMV,KAAK,CAACW,GAAN,CAAUV,OAAV,CAAlB;MACA,KAAKK,KAAL,GAAaI,GAAG,CAACR,IAAjB;IACF,CAHA,CAGE,OAAMU,CAAN,EAAQ;MACRC,OAAO,CAACC,KAAR,CAAcF,CAAd;IACF;EACD,CAnBY;;EAoBbG,OAAO,EAAE;IACPC,UAAU,GAAG;MACX,KAAKX,WAAL,GAAmB,IAAnB;;MACA,IACE,KAAKF,KAAL,KAAe,EAAf,IACA,CAAC,KAAKA,KAAL,CAAWc,QAAX,CAAoB,GAApB,CADD,IAEA,KAAKb,QAAL,CAAcc,MAAd,GAAuB,CAHzB,EAIE;QACA,KAAKb,WAAL,GAAmB,KAAnB;QACA;MACF;IAED,CAZM;;IAcL,MAAMc,OAAN,GAAgB;MAChB,MAAMT,GAAE,GAAI,MAAMV,KAAK,CAACoB,IAAN,CAAWnB,OAAX,EAAoB;QAAEE,KAAK,EAAE,KAAKA,KAAd;QAAqBC,QAAQ,EAAE,KAAKA;MAApC,CAApB,CAAlB;MACA,KAAKE,KAAL,GAAa,CAAC,GAAG,KAAKA,KAAT,EAAgBI,GAAG,CAACR,IAApB,CAAb;MACA,KAAKE,QAAL,GAAgB,EAAhB;MACA,KAAKD,KAAL,GAAa,EAAb;;MAEA,IAAI,KAAKC,QAAL,KAAkB,SAAlB,IAA+B,KAAKA,QAAL,KAAkB,QAAjD,IAA6D,KAAKA,QAAL,KAAkB,MAAnF,EAA0F;QACxF,KAAKI,SAAL,GAAiB,IAAjB;MACF,CAFA,MAEO;QACL,KAAKD,SAAL,IAAkB,IAAlB;QACAc,MAAM,CAACC,QAAP,CAAgBC,IAAhB,GAAuB,QAAvB;MACF;IACF;;EA1BO;AApBI,CAAf","names":["backgroundUrl","axios","baseURL","data","email","username","formIsValid","users","userAdded","nameTaken","created","res","get","e","console","error","methods","onRegister","includes","length","addUser","post","window","location","href"],"sourceRoot":"","sources":["/Users/justinaho1996/yoobee_JS_Framework_Assignment/src/views/RegisterView.vue"],"sourcesContent":["<template>\n  <div\n    class=\"background-image\"\n    :style=\"{ backgroundImage: 'url(' + backgroundUrl + ')' }\"\n  >\n    <div class=\"register\">\n      <h1>Sign Up</h1>\n      <form class=\"registerForm\" @submit.prevent=\"onRegister\" novalidate>\n        <div class=\"registerForm__container\">\n          <input\n            class=\"registerFormInput\"\n            type=\"email\"\n            placeholder=\"Email Address\"\n            name=\"email\"\n            v-model=\"email\"\n            required\n          />\n          <input\n            class=\"registerFormInput\"\n            type=\"username\"\n            placeholder=\"Username\"\n            name=\"username\"\n            v-model=\"username\" @keyup.enter=\"addUser\"\n            required\n          />\n\n          <button type=\"submit\" id=\"submitSignUp\">Sign Up</button>\n        </div>\n\n        <div class=\"registerForm__container\">\n          <span class=\"account\"\n            ><a href=\"/login\">Already have an account?</a></span\n          >\n        </div>\n      </form>\n    </div>\n    <div v-if=\"!formIsValid\">Please enter a valid email and username</div>\n     <div v-if=\"nameTaken\">This username has been taken, please try another one.</div>\n  </div>\n</template>\n\n<style>\n@import \"../assets/styles/main.css\";\n</style>\n\n<script>\nimport backgroundUrl from \"../assets/images/LogInSignUpDesktop.jpg\";\nimport axios from \"axios\";\nconst baseURL = \"http://localhost:3000/users\"\n\nexport default {\n  data() {\n    return {\n      backgroundUrl,\n      email: '',\n      username: '',\n      formIsValid: true,\n      users: [],\n      userAdded: false,\n      nameTaken: false\n    };\n  },\n   async created(){\n    try {\n      const res = await axios.get(baseURL);\n      this.users = res.data;\n    } catch(e){\n      console.error(e);\n    }\n  },\n  methods: {\n    onRegister() {\n      this.formIsValid = true;\n      if (\n        this.email === \"\" ||\n        !this.email.includes(\"@\") ||\n        this.username.length < 1\n      ) {\n        this.formIsValid = false;\n        return;\n      }\n      \n    },\n\n      async addUser() {\n      const res = await axios.post(baseURL, { email: this.email, username: this.username });\n      this.users = [...this.users, res.data];\n      this.username = '';\n      this.email = '';\n\n      if (this.username === 'justina' || this.username === 'keegan' || this.username === 'thom'){\n        this.nameTaken = true;\n      } else {\n        this.userAdded == true\n        window.location.href = \"/login\";\n      } \n    }\n    \n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}