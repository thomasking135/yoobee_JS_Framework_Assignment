{"ast":null,"code":"import backgroundUrl from \"../assets/images/LogInSignUpDesktop.jpg\";\nimport axios from \"axios\";\nconst baseURL = \"http://localhost:3000/users\";\nexport default {\n  data() {\n    return {\n      backgroundUrl,\n      email: '',\n      username: '',\n      formIsValid: true,\n      users: []\n    };\n  },\n\n  async created() {\n    try {\n      const res = await axios.get(baseURL);\n      this.users = res.data;\n    } catch (e) {\n      console.error(e);\n    }\n  },\n\n  methods: {\n    onRegister() {\n      this.formIsValid = true;\n\n      if (this.email === \"\" || !this.email.includes(\"@\") || this.username.length < 1) {\n        this.formIsValid = false;\n        return;\n      }\n    },\n\n    async addUser() {\n      const res = await axios.post(baseURL, {\n        email: this.email,\n        username: this.username\n      });\n      this.users = [...this.users, res.data];\n      this.username = '';\n      this.email = '';\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA8CA,OAAOA,aAAP,MAA0B,yCAA1B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,MAAMC,OAAM,GAAI,6BAAhB;AAEA,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLH,aADK;MAELI,KAAK,EAAE,EAFF;MAGLC,QAAQ,EAAE,EAHL;MAILC,WAAW,EAAE,IAJR;MAKLC,KAAK,EAAE;IALF,CAAP;EAOD,CATY;;EAUZ,MAAMC,OAAN,GAAe;IACd,IAAI;MACF,MAAMC,GAAE,GAAI,MAAMR,KAAK,CAACS,GAAN,CAAUR,OAAV,CAAlB;MACA,KAAKK,KAAL,GAAaE,GAAG,CAACN,IAAjB;IACF,CAHA,CAGE,OAAMQ,CAAN,EAAQ;MACRC,OAAO,CAACC,KAAR,CAAcF,CAAd;IACF;EACD,CAjBY;;EAkBbG,OAAO,EAAE;IACPC,UAAU,GAAG;MACX,KAAKT,WAAL,GAAmB,IAAnB;;MACA,IACE,KAAKF,KAAL,KAAe,EAAf,IACA,CAAC,KAAKA,KAAL,CAAWY,QAAX,CAAoB,GAApB,CADD,IAEA,KAAKX,QAAL,CAAcY,MAAd,GAAuB,CAHzB,EAIE;QACA,KAAKX,WAAL,GAAmB,KAAnB;QACA;MACF;IAED,CAZM;;IAcL,MAAMY,OAAN,GAAgB;MAChB,MAAMT,GAAE,GAAI,MAAMR,KAAK,CAACkB,IAAN,CAAWjB,OAAX,EAAoB;QAAEE,KAAK,EAAE,KAAKA,KAAd;QAAqBC,QAAQ,EAAE,KAAKA;MAApC,CAApB,CAAlB;MACA,KAAKE,KAAL,GAAa,CAAC,GAAG,KAAKA,KAAT,EAAgBE,GAAG,CAACN,IAApB,CAAb;MACA,KAAKE,QAAL,GAAgB,EAAhB;MACA,KAAKD,KAAL,GAAa,EAAb;IACF;;EAnBO;AAlBI,CA