{"ast":null,"code":"import Axios from \"axios\";\nimport ProfileForm from \"@/components/ProfileForm.vue\";\nimport FooterSection from \"@/components/FooterSection.vue\";\nconst baseURL = \"http://localhost:3000/users\";\nconst cloudName = \"dqyhsobqa\";\nconst unsignedUploadPreset = \"ewvntos9\";\nconst cloudinaryLink = `https://api.cloudinary.com/v1_1/${cloudName}/upload`;\nexport default {\n  name: \"App\",\n  components: {\n    ProfileForm,\n    FooterSection\n  },\n\n  data() {\n    return {\n      src: \"\",\n      uploading: false,\n      username: \"\"\n    };\n  },\n\n  async created() {\n    try {\n      const res = await Axios.get(baseURL);\n      this.users = res.data;\n    } catch (e) {\n      console.error(e);\n    }\n  },\n\n  methods: {\n    uploadFile(fileData) {\n      this.uploading = true;\n      const formData = new FormData();\n      formData.append(\"file\", fileData);\n      formData.append(\"upload_preset\", unsignedUploadPreset);\n      Axios.post(cloudinaryLink, formData).then(res => {\n        this.src = res.data.url;\n        this.uploading = false;\n      });\n    },\n\n    removeImage() {\n      this.src = \"\";\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAyDA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,WAAP,MAAwB,8BAAxB;AACA,OAAOC,aAAP,MAA0B,gCAA1B;AAEA,MAAMC,OAAM,GAAI,6BAAhB;AACA,MAAMC,SAAQ,GAAI,WAAlB;AACA,MAAMC,oBAAmB,GAAI,UAA7B;AACA,MAAMC,cAAa,GAAK,mCAAkCF,SAAU,SAApE;AAEA,eAAe;EACbG,IAAI,EAAE,KADO;EAEbC,UAAU,EAAE;IACVP,WADU;IAEVC;EAFU,CAFC;;EAMbO,IAAI,GAAG;IACL,OAAO;MACLC,GAAG,EAAE,EADA;MAELC,SAAS,EAAE,KAFN;MAGLC,QAAQ,EAAE;IAHL,CAAP;EAKD,CAZY;;EAab,MAAMC,OAAN,GAAgB;IACd,IAAI;MACF,MAAMC,GAAE,GAAI,MAAMd,KAAK,CAACe,GAAN,CAAUZ,OAAV,CAAlB;MAEA,KAAKa,KAAL,GAAaF,GAAG,CAACL,IAAjB;IACF,CAJA,CAIE,OAAOQ,CAAP,EAAU;MACVC,OAAO,CAACC,KAAR,CAAcF,CAAd;IACF;EACD,CArBY;;EAsBbG,OAAO,EAAE;IACPC,UAAU,CAACC,QAAD,EAAW;MACnB,KAAKX,SAAL,GAAiB,IAAjB;MACA,MAAMY,QAAO,GAAI,IAAIC,QAAJ,EAAjB;MACAD,QAAQ,CAACE,MAAT,CAAgB,MAAhB,EAAwBH,QAAxB;MACAC,QAAQ,CAACE,MAAT,CAAgB,