{"ast":null,"code":"import backgroundUrl from \"../assets/images/LogInSignUpDesktop.jpg\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      backgroundUrl,\n      email: '',\n      password: '',\n      confirmPassword: ''\n    };\n  },\n\n  methods: {\n    onCreatePost() {\n      axios.post('https://jsonplaceholder.typicode.com/users', {\n        email: this.email,\n        password: this.password\n      }).then(response => {\n        this.isSuccess = true;\n        console.log(response);\n        console.log(this.email + ' ' + this.password); //if login form is validated, this directs the user to the landing page.\n\n        if (this.isSuccess == true) {\n          window.location.href = \"/\";\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA6BA,OAAOA,aAAP,MAA0B,yCAA1B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLF,aADK;MAELG,KAAK,EAAC,EAFD;MAGLC,QAAQ,EAAC,EAHJ;MAILC,eAAe,EAAC;IAJX,CAAP;EAMD,CARY;;EASbC,OAAO,EAAE;IACLC,YAAY,GAAG;MACbN,KAAK,CAACO,IAAN,CAAW,4CAAX,EACA;QAACL,KAAK,EAAE,KAAKA,KAAb;QAAoBC,QAAQ,EAAE,KAAKA;MAAnC,CADA,EAEEK,IAFF,CAEOC,QAAO,IAAK;QACjB,KAAKC,SAAL,GAAiB,IAAjB;QACAC,OAAO,CAACC,GAAR,CAAYH,QAAZ;QACAE,OAAO,CAACC,GAAR,CAAY,KAAKV,KAAL,GAAa,GAAb,GAAmB,KAAKC,QAApC,EAHiB,CAKjB;;QACA,IAAI,KAAKO,SAAL,IAAkB,IAAtB,EAA2B;UACzBG,MAAM,CAACC,QAAP,CAAgBC,IAAhB,GAAuB,GAAvB;QACF;MACD,CAXD;IAYF;;EAdK;AATI,CAAf","names":["backgroundUrl","axios","data","email","password","confirmPassword","methods","onCreatePost","post","then","response","isSuccess","console","log","window","location","href"],"sourceRoot":"","sources":["/Users/justinaho1996/yoobee_JS_Framework_Assignment/src/views/RegisterView.vue"],"sourcesContent":["<template>\n  <div class=\"background-image\" :style=\"{ backgroundImage: 'url(' + backgroundUrl + ')' }\">\n    <div class=\"register\">\n      <h1>Sign Up</h1>  \n      <form class=\"registerForm\">\n        <div class=\"registerForm__container\">\n          <input class=\"registerFormInput\" type=\"text\" placeholder=\"Email Address\" name=\"email\" required>\n          <input class=\"registerFormInput\" type=\"password\" placeholder=\"Password\" name=\"psw\" required>\n          <input class=\"registerFormInput\" type=\"password\" placeholder=\"Confirm Password\" name=\"psw\" required>\n              \n          <button type=\"submit\" id=\"submitSignUp\">Sign Up</button>\n        </div>\n\n        <div class=\"registerForm__container\">\n           <span class=\"account\"><a href=\"/login\">Already have an account?</a></span>\n        </div>\n        \n      </form>\n    </div>\n  </div>\n</template>\n\n\n\n<style>\n@import '../assets/styles/main.css';\n</style>\n\n<script>\nimport backgroundUrl from \"../assets/images/LogInSignUpDesktop.jpg\";\nimport axios from 'axios';\n\nexport default {\n  data() {\n    return {\n      backgroundUrl,\n      email:'',\n      password:'',\n      confirmPassword:''\n    };\n  },\n  methods: {\n      onCreatePost() {\n        axios.post('https://jsonplaceholder.typicode.com/users',\n        {email: this.email, password: this.password },\n        ).then(response => {\n          this.isSuccess = true;\n          console.log(response);\n          console.log(this.email + ' ' + this.password);\n\n          //if login form is validated, this directs the user to the landing page.\n          if (this.isSuccess == true){\n            window.location.href = \"/\";\n          }\n        });\n      }\n\n    }\n};\n</script>"]},"metadata":{},"sourceType":"module"}