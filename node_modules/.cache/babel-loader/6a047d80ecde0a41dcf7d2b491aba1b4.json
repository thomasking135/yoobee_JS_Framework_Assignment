{"ast":null,"code":"import backgroundUrl from \"../assets/images/LogInSignUpDesktop.jpg\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      backgroundUrl,\n      email: '',\n      username: '',\n      loginSuccess: false\n    };\n  },\n\n  methods: {\n    onLogin() {\n      axios.post('https://jsonplaceholder.typicode.com/users', {\n        email: this.email,\n        username: this.username\n      }).then(response => {\n        this.loginSuccess = true;\n        console.log(response);\n        console.log(this.email + ' ' + this.username); //if login form is validated, this directs the user to the landing page.\n\n        if (this.loginSuccess == true) {\n          window.location.href = \"/\";\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA+BE,OAAOA,aAAP,MAA0B,yCAA1B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLF,aADK;MAELG,KAAK,EAAC,EAFD;MAGLC,QAAQ,EAAC,EAHJ;MAILC,YAAY,EAAE;IAJT,CAAP;EAMD,CARY;;EASbC,OAAO,EAAE;IACPC,OAAO,GAAG;MACRN,KAAK,CAACO,IAAN,CAAW,4CAAX,EACA;QAACL,KAAK,EAAE,KAAKA,KAAb;QAAoBC,QAAQ,EAAE,KAAKA;MAAnC,CADA,EAEEK,IAFF,CAEOC,QAAO,IAAK;QACjB,KAAKL,YAAL,GAAoB,IAApB;QACAM,OAAO,CAACC,GAAR,CAAYF,QAAZ;QACAC,OAAO,CAACC,GAAR,CAAY,KAAKT,KAAL,GAAa,GAAb,GAAmB,KAAKC,QAApC,EAHiB,CAKjB;;QACA,IAAI,KAAKC,YAAL,IAAqB,IAAzB,EAA8B;UAC5BQ,MAAM,CAACC,QAAP,CAAgBC,IAAhB,GAAuB,GAAvB;QACF;MACD,CAXD;IAYF;;EAdO;AATI,CAAf","names":["backgroundUrl","axios","data","email","username","loginSuccess","methods","onLogin","post","then","response","console","log","window","location","href"],"sourceRoot":"","sources":["/Users/justinaho1996/yoobee_JS_Framework_Assignment/src/views/LoginView.vue"],"sourcesContent":["<template>\n  <div class=\"background-image\" :style=\"{ backgroundImage: 'url(' + backgroundUrl + ')' }\">\n    <div class=\"login\">\n    <h1>Login</h1>  \n\n    <form class=\"loginForm\" @submit.prevent=\"onLogin\">\n      <div class=\"loginForm__container\">\n        <input class=\"loginFormInput\" type=\"email\" placeholder=\"Email Address\" name=\"email\" v-model=\"email\" required>\n        <input class=\"loginFormInput\" type=\"text\" placeholder=\"Username\" name=\"username\" v-model=\"username\" required>\n            \n        <button type=\"submit\" id=\"submitLogin\">Login</button>\n      </div>\n\n      <div class=\"loginForm__container\">\n        <span class=\"signUp\"><a href=\"/register\">Don't have an account?</a></span>\n        <span class=\"psw\"><a href=\"/register\">Forgot password?</a></span>\n      </div>\n    </form>\n    </div>\n    <div class=\"alert\" v-if=\"loginSuccess\">Logged in successfully</div>\n  </div>\n</template>\n\n\n\n<style>\n\n@import '../assets/styles/main.css';\n</style>\n\n<script>\n  import backgroundUrl from \"../assets/images/LogInSignUpDesktop.jpg\";\n  import axios from 'axios';\n  export default {\n    data() {\n      return {\n        backgroundUrl,\n        email:'',\n        username:'',\n        loginSuccess: false,\n      };\n    },\n    methods: {\n      onLogin() {\n        axios.post('https://jsonplaceholder.typicode.com/users',\n        {email: this.email, username: this.username },\n        ).then(response => {\n          this.loginSuccess = true;\n          console.log(response);\n          console.log(this.email + ' ' + this.username);\n\n          //if login form is validated, this directs the user to the landing page.\n          if (this.loginSuccess == true){\n            window.location.href = \"/\";\n          }\n        });\n      }\n\n    }\n  };\n</script>"]},"metadata":{},"sourceType":"module"}