{"ast":null,"code":"import Vue from 'vue';\nimport axios from 'axios';\nimport VueAxios from 'vue-axios';\nVue.use(VueAxios, axios);\nconst app = Vue.createApp({});\napp.mount('#app');\nexport default {\n  name: \"comments\",\n\n  data() {\n    return {\n      comments: null\n    };\n  },\n\n  methods: {\n    getData() {\n      this.axios.get(\"http://localhost:3000/comments\").then(response => {\n        this.users = response.data;\n        console.log(response);\n        this.comments = response.data;\n      });\n    },\n\n    deleteUser(id) {\n      this.axios.delete(\"http://localhost:3000/comments\" + id).then(() => {\n        this.getData();\n      });\n    }\n\n  },\n\n  mounted() {\n    this.getData();\n  }\n\n};","map":{"version":3,"mappings":"AAkBA,OAAOA,GAAP,MAAgB,KAAhB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,OAAOC,QAAP,MAAqB,WAArB;AAEAF,GAAG,CAACG,GAAJ,CAAQD,QAAR,EAAkBD,KAAlB;AAEA,MAAMG,GAAE,GAAIJ,GAAG,CAACK,SAAJ,CAAc,EAAd,CAAZ;AAIAD,GAAG,CAACE,KAAJ,CAAU,MAAV;AACA,eAAe;EACbC,IAAI,EAAE,UADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,QAAQ,EAAE;IADL,CAAP;EAGD,CANY;;EAQbC,OAAO,EAAE;IACPC,OAAO,GAAG;MACR,KAAKV,KAAL,CAAWW,GAAX,CAAe,gCAAf,EAAiDC,IAAjD,CAAuDC,QAAD,IAAc;QAClE,KAAKC,KAAL,GAAaD,QAAQ,CAACN,IAAtB;QACAQ,OAAO,CAACC,GAAR,CAAYH,QAAZ;QACA,KAAKL,QAAL,GAAgBK,QAAQ,CAACN,IAAzB;MACD,CAJD;IAKD,CAPM;;IAQPU,UAAU,CAACC,EAAD,EAAK;MACX,KAAKlB,KAAL,CAAWmB,MAAX,CAAkB,mCAAiCD,EAAnD,EAAuDN,IAAvD,CAA4D,MAAM;QAC9D,KAAKF,OAAL;MACL,CAFC;IAGJ;;EAZO,CARI;;EAsBbU,OAAO,GAAG;IACR,KAAKV,OAAL;EACD;;AAxBY,CAAf","names":["Vue","axios","VueAxios","use","app","createApp","mount","name","data","comments","methods","getData","get","then","response","users","console","log","deleteUser","id","delete","mounted"],"sourceRoot":"","sources":["/Users/justinaho1996/yoobee_JS_Framework_Assignment/src/components/Users.vue"],"sourcesContent":["<template>\n  <div>\n     <table class=\"commentPosts\" border=\"1px\">\n      <tr>\n          <td>name</td>\n          <td>post</td>\n          <td>action</td>\n      </tr>\