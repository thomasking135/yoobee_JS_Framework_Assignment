{"ast":null,"code":"import backgroundUrl from \"../assets/images/LogInSignUpDesktop.jpg\";\nimport axios from \"axios\";\nimport userData from '/data/db.json';\nconst baseURL = \"http://localhost:3000/users\";\nexport default {\n  name: 'login',\n\n  data() {\n    return {\n      backgroundUrl,\n      email: \"\",\n      username: \"\",\n      user: userData,\n      users: [],\n      url: 'http://localhost:3000/users',\n      formIsValid: true,\n      userDetailsIsValid: true\n    };\n  },\n\n  mounted() {\n    axios.get(this.url).then(response => {\n      this.user = response.data;\n    });\n  },\n\n  methods: {\n    submitForm() {\n      this.formIsValid = true;\n\n      if (this.email === \"\" || !this.email.includes(\"@\") || this.username.length < 1) {\n        this.formIsValid = false;\n        return;\n      }\n\n      console.log(this.users[0].username);\n\n      for (let i = 0; i <= this.users.length; i++) {\n        if (this.username == this.users[i].username) {\n          console.log(i);\n          window.location.href = \"/\";\n        }\n\n        if (this.email == this.users[i].email) {\n          console.log(i);\n          window.location.href = \"/\";\n        } else {\n          this.userDetailsIsValid = false;\n          return;\n        }\n      } //GET\n\n\n      axios.get(baseURL).then(res => {\n        handleResult(res);\n      }).catch(err => console.log(err));\n\n      function handleResult(data) {\n        console.log(data);\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA+CA,OAAOA,aAAP,MAA0B,yCAA1B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,OAAOC,QAAP,MAAqB,eAArB;AACA,MAAMC,OAAM,GAAI,6BAAhB;AACA,eAAe;EACbC,IAAI,EAAE,OADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLL,aADK;MAELM,KAAK,EAAE,EAFF;MAGLC,QAAQ,EAAE,EAHL;MAILC,IAAI,EAAEN,QAJD;MAKLO,KAAK,EAAE,EALF;MAMLC,GAAG,EAAE,6BANA;MAOLC,WAAW,EAAE,IAPR;MAQLC,kBAAkB,EAAE;IARf,CAAP;EAUD,CAbY;;EAcbC,OAAO,GAAG;IACRZ,KAAK,CAACa,GAAN,CAAU,KAAKJ,GAAf,EAAoBK,IAApB,CAA0BC,QAAD,IAAc;MACrC,KAAKR,IAAL,GAAYQ,QAAQ,CAACX