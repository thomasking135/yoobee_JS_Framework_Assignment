{"ast":null,"code":"import backgroundUrl from \"../assets/images/LogInSignUpDesktop.jpg\";\nimport axios from \"axios\";\nconst baseURL = \"http://localhost:3000/users\";\nexport default {\n  data() {\n    return {\n      backgroundUrl,\n      email: \"\",\n      username: \"\",\n      formIsValid: true\n    };\n  },\n\n  mounted() {\n    fetch(baseURL).then(res => res.json()).then(data => this.users = data).catch(err => console.log(err.message));\n  },\n\n  methods: {\n    submitForm() {\n      this.formIsValid = true;\n\n      if (this.email === \"\" || !this.email.includes(\"@\") || this.username.length < 1) {\n        this.formIsValid = false;\n        return;\n      } //GET\n\n\n      axios.get(baseURL).then(res => {\n        handleResult(res);\n      }).catch(err => console.log(err));\n\n      function handleResult(data) {\n        console.log(data);\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA8CA,OAAOA,aAAP,MAA0B,yCAA1B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,MAAMC,OAAM,GAAI,6BAAhB;AACA,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLH,aADK;MAELI,KAAK,EAAE,EAFF;MAGLC,QAAQ,EAAE,EAHL;MAILC,WAAW,EAAE;IAJR,CAAP;EAMD,CARY;;EASbC,OAAO,GAAG;IACRC,KAAK,CAACN,OAAD,CAAL,CACCO,IADD,CACMC,GAAE,IAAKA,GAAG,CAACC,IAAJ,EADb,EAECF,IAFD,CAEMN,IAAG,IAAK,KAAKS,KAAL,GAAaT,IAF3B,EAGCU,KAHD,CAGOC,GAAE,IAAKC,OAAO,CAACC,GAAR,CAAYF,GAAG,CAACG,OAAhB,CAHd;EAID,CAdY;;EAebC,OAAO,EAAE;IACPC,UAAU,GAAG;MACX,KAAKb,WAAL,GAAmB,IAAnB;;MACA,IACE,KAAKF,KAAL,KAAe,EAAf,IACA,CAAC,KAAKA,KAAL,CAAWgB,QAAX,CAAoB,GAApB,CADD,IAEA,KAAKf,QAAL,CAAcgB,MAAd,GAAuB,CAHzB,EAIE;QACA,KAAKf,WAAL,GAAmB,KAAnB;QACA;MACF,CATW,CAWX;;;MACAL,KAAI,CACDqB,GADH,CACOpB,OADP,EAEGO,IAFH,CAESC,GAAD,IAAS;QACba,YAAY,CAACb,GAAD,CAAZ;MACD,CAJH,EAKGG,KALH,CAKUC,GAAD,IAASC,OAAO,CAACC,GAAR,CAAYF,GAAZ,CALlB;;MAOA,SAASS,YAAT,CAAsBpB,IAAtB,EAA4B;QAC1BY,OAAO,CAACC,GAAR,CAAYb,IAAZ;MACF;IAEF;;EAxBO;AAfI,CAAf","names":["backgroundUrl","axios","baseURL","data","email","username","formIsValid","mounted","fetch","then","res","json","users","catch","err","console","log","message","methods","submitForm","includes","length","get","handleResult"],"sourceRoot":"","sources":["C:\\yoobee_JS_Framework_Assignment\\src\\views\\LoginView.vue"],"sourcesContent":["<template>\r\n  <div\r\n    class=\"background-image\"\r\n    :style=\"{ backgroundImage: 'url(' + backgroundUrl + ')' }\"\r\n  >\r\n    <div class=\"login\">\r\n      <h1>Login</h1>\r\n\r\n      <form class=\"loginForm\" @submit.prevent=\"submitForm\" novalidate>\r\n        <div class=\"loginForm__container\">\r\n          <input\r\n            class=\"loginFormInput\"\r\n            type=\"email\"\r\n            placeholder=\"Email Address\"\r\n            name=\"email\"\r\n            v-model.trim=\"email\"\r\n            required\r\n          />\r\n          <input\r\n            class=\"loginFormInput\"\r\n            type=\"text\"\r\n            placeholder=\"Username\"\r\n            name=\"username\"\r\n            v-model.trim=\"username\"\r\n            required\r\n          />\r\n          <button type=\"submit\" id=\"submitLogin\">Login</button>\r\n        </div>\r\n\r\n        <div class=\"loginForm__container\">\r\n          <span class=\"signUp\"\r\n            ><a href=\"/register\">Don't have an account?</a></span\r\n          >\r\n        </div>\r\n      </form>\r\n    </div>\r\n    <div v-if=\"!formIsValid\">Please enter a valid email and username</div>\r\n\r\n  </div>\r\n</template>\r\n\r\n<style>\r\n@import \"../assets/styles/main.css\";\r\n</style>\r\n\r\n<script>\r\nimport backgroundUrl from \"../assets/images/LogInSignUpDesktop.jpg\";\r\nimport axios from \"axios\";\r\nconst baseURL = \"http://localhost:3000/users\"\r\nexport default {\r\n  data() {\r\n    return {\r\n      backgroundUrl,\r\n      email: \"\",\r\n      username: \"\",\r\n      formIsValid: true,\r\n    };\r\n  },\r\n  mounted() {\r\n    fetch(baseURL)\r\n    .then(res => res.json())\r\n    .then(data => this.users = data)\r\n    .catch(err => console.log(err.message))\r\n  },\r\n  methods: {\r\n    submitForm() {\r\n      this.formIsValid = true;\r\n      if (\r\n        this.email === \"\" ||\r\n        !this.email.includes(\"@\") ||\r\n        this.username.length < 1\r\n      ) {\r\n        this.formIsValid = false;\r\n        return;\r\n      }\r\n\r\n      //GET\r\n      axios\r\n        .get(baseURL)\r\n        .then((res) => {\r\n          handleResult(res);\r\n        })\r\n        .catch((err) => console.log(err));\r\n\r\n      function handleResult(data) {\r\n        console.log(data);\r\n      }\r\n      \r\n    }\r\n  },\r\n};\r\n</script>\r\n"]},"metadata":{},"sourceType":"module"}