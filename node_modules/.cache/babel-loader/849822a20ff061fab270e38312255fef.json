{"ast":null,"code":"import backgroundUrl from \"../assets/images/LogInSignUpDesktop.jpg\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      backgroundUrl,\n      email: '',\n      username: '',\n      formIsValid: true\n    };\n  },\n\n  async created() {\n    try {\n      const res = await axios.get(baseURL);\n      this.users = res.data;\n    } catch (e) {\n      console.error(e);\n    }\n  },\n\n  methods: {\n    onRegister() {\n      this.formIsValid = true;\n\n      if (this.email === \"\" || !this.email.includes(\"@\") || this.username.length < 1) {\n        this.formIsValid = false;\n        return;\n      }\n\n      axios.post(\"http://localhost:3000/users\", {\n        email: this.email,\n        username: this.username\n      }).then(response => {\n        console.log(response);\n      }, error => {\n        console.log(error);\n      });\n    },\n\n    async addUser() {\n      const res = await axios.post(baseURL, {\n        name: this.userName\n      });\n      this.users = [...this.users, res.data];\n      this.userName = '';\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA6CA,OAAOA,aAAP,MAA0B,yCAA1B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLF,aADK;MAELG,KAAK,EAAE,EAFF;MAGLC,QAAQ,EAAE,EAHL;MAILC,WAAW,EAAE;IAJR,CAAP;EAMD,CARY;;EASZ,MAAMC,OAAN,GAAe;IACd,IAAI;MACF,MAAMC,GAAE,GAAI,MAAMN,KAAK,CAACO,GAAN,CAAUC,OAAV,CAAlB;MACA,KAAKC,KAAL,GAAaH,GAAG,CAACL,IAAjB;IACF,CAHA,CAGE,OAAMS,CAAN,EAAQ;MACRC,OAAO,CAACC,KAAR,CAAcF,CAAd;IACF;EACD,CAhBY;;EAiBbG,OAAO,EAAE;IACPC,UAAU,GAAG;MACX,KAAKV,WAAL,GAAmB,IAAnB;;MACA,IACE,KAAKF,KAAL,KAAe,EAAf,IACA,CAAC,KAAKA,KAAL,CAAWa,QAAX,CAAoB,GAApB,CADD,IAEA,KAAKZ,QAAL,CAAca,MAAd,GAAuB,CAHzB,EAIE;QACA,KAAKZ,WAAL,GAAmB,KAAnB;QACA;MACF;;MAEAJ,KAAI,CACDiB,IADH,CACQ,6BADR,EACuC;QACnCf,KAAK,EAAE,KAAKA,KADuB;QAEnCC,QAAQ,EAAE,KAAKA;MAFoB,CADvC,EAKGe,IALH,CAMKC,QAAD,IAAc;QACZR,OAAO,CAACS,GAAR,CAAYD,QAAZ;MACD,CARL,EASKP,KAAD,IAAW;QACTD,OAAO,CAACS,GAAR,CAAYR,KAAZ;MACF,CAXJ;IAaD,CAzBM