  backgroundUrl,\n      email: '',\n      username: '',\n      formIsValid: true,\n    };\n  },\n   async created(){\n    try {\n      const res = await axios.get(baseURL);\n      this.users = res.data;\n    } catch(e){\n      console.error(e);\n    }\n  },\n  methods: {\n    onRegister() {\n      this.formIsValid = true;\n      if (\n        this.email === \"\" ||\n        !this.email.includes(\"@\") ||\n        this.username.length < 1\n      ) {\n        this.formIsValid = false;\n        return;\n      }\n\n      axios\n        .post(\"http://localhost:3000/users\", {\n          email: this.email,\n          username: this.username,\n        })\n        .then(\n          (response) => {\n            console.log(response);\n          },\n          (error) => {\n            console.log(error);\n          }\n        );\n    },\n       async addUser() {\n        const res = await axios.post(baseURL, { name: this.userName });\n        this.users = [...this.users, res.data];\n        this.userName = '';\n      }\n    \n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}