{"ast":null,"code":"import backgroundUrl from \"../assets/images/LogInSignUpDesktop.jpg\";\nimport axios from \"axios\";\nimport userData from '/data/db.json';\nconst baseURL = \"http://localhost:3000/users\";\nexport default {\n  name: 'login',\n\n  data() {\n    return {\n      backgroundUrl,\n      email: \"\",\n      username: \"\",\n      users: userData,\n      url: 'http://localhost:3000/users',\n      formIsValid: true\n    };\n  },\n\n  mounted() {\n    axios.get(this.url).then(response => {\n      this.users = response.data;\n    });\n  },\n\n  methods: {\n    submitForm() {\n      this.formIsValid = true;\n\n      if (this.email === \"\" || !this.email.includes(\"@\") || this.username.length < 1) {\n        this.formIsValid = false;\n        return;\n      }\n\n      for (let i = 0; i <= this.users.length; i++) {\n        if (this.username == this.users[i].username) {\n          console.log('Username is correct');\n        }\n\n        if (this.email == this.users[i].email) {\n          console.log('Email is correct');\n        }\n      } //GET\n\n\n      axios.get(baseURL).then(res => {\n        handleResult(res);\n      }).catch(err => console.log(err));\n\n      function handleResult(data) {\n        console.log(data);\n      } // window.location.href = \"/\";\n\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAuGA,OAAOA,aAAP,MAA0B,yCAA1B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,OAAOC,QAAP,MAAqB,eAArB;AACA,MAAMC,OAAM,GAAI,6BAAhB;AACA,eAAe;EACbC,IAAI,EAAE,OADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLL,aADK;MAELM,KAAK,EAAE,EAFF;MAGLC,QAAQ,EAAE,EAHL;MAILC,KAAK,EAAEN,QAJF;MAKLO,GAAG,EAAE,6BALA;MAMLC,WAAW,EAAE;IANR,CAAP;EAQD,CAXY;;EAYbC,OAAO,GAAG;IACRV,KAAK,CAACW,GAAN,CAAU,KAAKH,GAAf,EAAoBI,IAApB,CAA0BC,QAAD,IAAc;MACrC,KAAKN,KAAL,GAAaM,QAAQ,CAACT,IAAtB;IACD,CAFD;EAGD,CAhBY;;EAiBbU,OAAO,EAAE;IACPC,UAAU,GAAG;MACX,KAAKN,WAAL,GAAmB,IAAnB;;MACA,IACE,KAAKJ,KAAL,KAAe,EAAf,IACA,CAAC,KAAKA,KAAL,CAAWW,QAAX,CAAoB,GAApB,CADD,IAEA,KAAKV,QAAL,CAAcW,MAAd,GAAuB,CAHzB,EAIE;QACA,KAAKR,WAAL,GAAmB,KAAnB;QACA;MACF;;MAEA,KAAK,IAAIS,CAAC,GAAC,CAAX,EAAcA,CAAC,IAAE,KAAKX,KAAL,CAAWU,MAA5B,EAAoCC,CAAC,EAArC,EAAyC;QACvC,IAAI,KAAKZ,QAAL,IAAiB,KAAKC,KAAL,CAAWW,CAAX,EAAcZ,QAAnC,EAA6C;UAC3Ca,OAAO,CAACC,GAAR,CAAY,qBAAZ;QACF;;QACA,IAAI,KAAKf,KAAL,IAAc,KAAKE,KAAL,CAAWW,CAAX,EAAcb,KAAhC,EAAsC;UACtCc,OAAO,CAACC,GAAR,CAAY,kBAAZ;QACA;MACF,CAlBW,CAoBX;;;MACApB,KAAI,CACDW,GADH,CACOT,OADP,EAEGU,IAFH,CAESS,GAAD,IAAS;QACbC,YAAY,CAACD,GAAD,CAAZ;MACD,CAJH,EAKGE,KALH,CAKUC,GAAD,IAASL,OAAO,CAACC,GAAR,CAAYI,GAAZ,CALlB;;MAOA,SAASF,YAAT,CAAsBlB,IAAtB,EAA4B;QAC1Be,OAAO,CAACC,GAAR,CAAYhB,IAAZ;MACF,CA9BW,CAgCX;;IACD;;EAlCM;AAjBI,CAAf","names":["backgroundUrl","axios","userData","baseURL","name","data","email","username","users","url","formIsValid","mounted","get","then","response","methods","submitForm","includes","length","i","console","log","res","handleResult","catch","err"],"sourceRoot":"","sources":["C:\\yoobee_JS_Framework_Assignment\\src\\views\\LoginView.vue"],"sourcesContent":["<template>\r\n  <div\r\n    class=\"background-image\"\r\n    :style=\"{ backgroundImage: 'url(' + backgroundUrl + ')' }\"\r\n  >\r\n    <div class=\"login\">\r\n      <h1>Login</h1>\r\n\r\n      <form class=\"loginForm\" @submit.prevent=\"submitForm\" novalidate>\r\n        <div class=\"loginForm__container\">\r\n          <input\r\n            class=\"loginFormInput\"\r\n            type=\"email\"\r\n            placeholder=\"Email Address\"\r\n            name=\"email\"\r\n            v-model.trim=\"email\"\r\n            required\r\n          />\r\n          <input\r\n            class=\"loginFormInput\"\r\n            type=\"text\"\r\n            placeholder=\"Username\"\r\n            name=\"username\"\r\n            v-model.trim=\"username\"\r\n            required\r\n          />\r\n          \r\n          <button type=\"submit\" id=\"submitLogin\">Login</button>\r\n        </div>\r\n\r\n        <div class=\"loginForm__container\">\r\n          <span class=\"signUp\"\r\n            ><a href=\"/register\">Don't have an account?</a></span\r\n          >\r\n        </div>\r\n        <br><br>\r\n        <div class=\"centering\">\r\n        <div class=\"loginForm__container\">\r\n          <span class=\"signUp\"\r\n            ><div class=\"tooltip\">Need answers?\r\n        <span class=\"tooltiptext\"><router-link to=\"/about\">Click here to learn more</router-link></span>\r\n        </div></span\r\n          >\r\n        </div>\r\n      </div>\r\n      </form>\r\n    </div>\r\n    <div v-if=\"!formIsValid\">Please enter a valid email and username</div>\r\n\r\n  </div>\r\n\r\n  \r\n\r\n</template>\r\n\r\n\r\n\r\n<style>\r\n@import \"../assets/styles/main.css\";\r\n\r\n.tooltip {\r\n  position: relative;\r\n  display: inline-block;\r\n  border-bottom: 1px dotted black;\r\n  margin-left:auto;\r\n  margin-right: auto;\r\n}\r\n\r\n.tooltip .tooltiptext {\r\n  visibility: hidden;\r\n  width: 120px;\r\n  background-color: #555;\r\n  color: #fff;\r\n  text-align: center;\r\n  border-radius: 6px;\r\n  padding: 5px 0;\r\n  position: absolute;\r\n  z-index: 1;\r\n  bottom: 125%;\r\n  left: 50%;\r\n  margin-left: -60px;\r\n  opacity: 0;\r\n  transition: opacity 0.3s;\r\n}\r\n\r\n.tooltip .tooltiptext::after {\r\n  content: \"\";\r\n  position: absolute;\r\n  top: 100%;\r\n  left: 50%;\r\n  margin-left: -5px;\r\n  border-width: 5px;\r\n  border-style: solid;\r\n  border-color: #555 transparent transparent transparent;\r\n}\r\n\r\n.tooltip:hover .tooltiptext {\r\n  visibility: visible;\r\n  opacity: 1;\r\n}\r\n</style>\r\n\r\n<script>\r\nimport backgroundUrl from \"../assets/images/LogInSignUpDesktop.jpg\";\r\nimport axios from \"axios\";\r\nimport userData from '/data/db.json'\r\nconst baseURL = \"http://localhost:3000/users\"\r\nexport default {\r\n  name: 'login',\r\n  data() {\r\n    return {\r\n      backgroundUrl,\r\n      email: \"\",\r\n      username: \"\",\r\n      users: userData,\r\n      url: 'http://localhost:3000/users',\r\n      formIsValid: true,\r\n    };\r\n  },\r\n  mounted() {\r\n    axios.get(this.url).then((response) => {\r\n      this.users = response.data\r\n    })\r\n  },\r\n  methods: {\r\n    submitForm() {\r\n      this.formIsValid = true;\r\n      if (\r\n        this.email === \"\" ||\r\n        !this.email.includes(\"@\") ||\r\n        this.username.length < 1\r\n      ) {\r\n        this.formIsValid = false;\r\n        return;\r\n      }\r\n\r\n      for (let i=0; i<=this.users.length; i++) {\r\n        if (this.username == this.users[i].username) {\r\n          console.log('Username is correct')\r\n        }\r\n        if (this.email == this.users[i].email){\r\n        console.log('Email is correct')\r\n        } \r\n      }\r\n\r\n      //GET\r\n      axios\r\n        .get(baseURL)\r\n        .then((res) => {\r\n          handleResult(res);\r\n        })\r\n        .catch((err) => console.log(err));\r\n\r\n      function handleResult(data) {\r\n        console.log(data);\r\n      }\r\n\r\n      // window.location.href = \"/\";\r\n    },\r\n  },\r\n};\r\n</script>\r\n"]},"metadata":{},"sourceType":"module"}