{"ast":null,"code":"import backgroundUrl from \"../assets/images/LogInSignUpDesktop.jpg\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      backgroundUrl,\n      email: \"\",\n      username: \"\",\n      formIsValid: true // loginSuccess: false,\n\n    };\n  },\n\n  methods: {\n    submitForm() {\n      this.formIsValid = true;\n\n      if (this.email === \"\" || !this.email.includes(\"@\") || this.username.length < 1) {\n        this.formIsValid = false;\n        return;\n      } //   // //GET\n\n\n      axios.get(\"http://localhost:3000/users\").then(res => {\n        handleResult(res);\n      }).catch(err => console.log(err));\n\n      function handleResult(data) {\n        console.log(data);\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA+CA,OAAOA,aAAP,MAA0B,yCAA1B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLF,aADK;MAELG,KAAK,EAAE,EAFF;MAGLC,QAAQ,EAAE,EAHL;MAILC,WAAW,EAAE,IAJR,CAKL;;IALK,CAAP;EAOD,CATY;;EAUbC,OAAO,EAAE;IACPC,UAAU,GAAG;MACX,KAAKF,WAAL,GAAmB,IAAnB;;MACA,IACE,KAAKF,KAAL,KAAe,EAAf,IACA,CAAC,KAAKA,KAAL,CAAWK,QAAX,CAAoB,GAApB,CADD,IAEA,KAAKJ,QAAL,CAAcK,MAAd,GAAuB,CAHzB,EAIE;QACA,KAAKJ,WAAL,GAAmB,KAAnB;QACA;MACF,CATW,CAUX;;;MACAJ,KAAI,CACDS,GADH,CACO,6BADP,EAEGC,IAFH,CAESC,GAAD,IAAS;QACbC,YAAY,CAACD,GAAD,CAAZ;MACD,CAJH,EAKGE,KALH,CAKUC,GAAD,IAASC,OAAO,CAACC,GAAR,CAAYF,GAAZ,CALlB;;MAOA,SAASF,YAAT,CAAsBX,IAAtB,EAA4B;QAC1Bc,OAAO,CAACC,GAAR,CAAYf,IAAZ;MACF;IACD;;EAtBM;AAVI,CAAf","names":["backgroundUrl","axios","data","email","username","formIsValid","methods","submitForm","includes","length","get","then","res","handleResult","catch","err","console","log"],"sourceRoot":"","sources":["/Users/justinaho1996/yoobee_JS_Framework_Assignment/src/views/LoginView.vue"],"sourcesContent":["<template>\n  <div\n    class=\"background-image\"\n    :style=\"{ backgroundImage: 'url(' + backgroundUrl + ')' }\"\n  >\n    <div class=\"login\">\n      <h1>Login</h1>\n\n      <form class=\"loginForm\" @submit.prevent=\"submitForm\" novalidate>\n        <div class=\"loginForm__container\">\n          <input\n            class=\"loginFormInput\"\n            type=\"email\"\n            placeholder=\"Email Address\"\n            name=\"email\"\n            v-model.trim=\"email\"\n            required\n          />\n          <input\n            class=\"loginFormInput\"\n            type=\"text\"\n            placeholder=\"Username\"\n            name=\"username\"\n            v-model.trim=\"username\"\n            required\n          />\n\n          <button type=\"submit\" id=\"submitLogin\">Login</button>\n        </div>\n\n        <div class=\"loginForm__container\">\n          <span class=\"signUp\"\n            ><a href=\"/register\">Don't have an account?</a></span\n          >\n        </div>\n      </form>\n    </div>\n    <!-- <div class=\"alert\" v-if=\"loginSuccess\">Logged in successfully</div> -->\n    <div v-if=\"!formIsValid\">Please enter a valid email and username</div>\n  </div>\n</template>\n\n<style>\n@import \"../assets/styles/main.css\";\n</style>\n\n<script>\nimport backgroundUrl from \"../assets/images/LogInSignUpDesktop.jpg\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      backgroundUrl,\n      email: \"\",\n      username: \"\",\n      formIsValid: true,\n      // loginSuccess: false,\n    };\n  },\n  methods: {\n    submitForm() {\n      this.formIsValid = true;\n      if (\n        this.email === \"\" ||\n        !this.email.includes(\"@\") ||\n        this.username.length < 1\n      ) {\n        this.formIsValid = false;\n        return;\n      }\n      //   // //GET\n      axios\n        .get(\"http://localhost:3000/users\")\n        .then((res) => {\n          handleResult(res);\n        })\n        .catch((err) => console.log(err));\n\n      function handleResult(data) {\n        console.log(data);\n      }\n    },\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}